{
  "name": "@autoloan/marketplace",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "ui:watch": "esbuild src/app/main.tsx --bundle --watch --outdir=src/public/assets --loader:.tsx=tsx --platform=browser --jsx=automatic",
    "build": "esbuild src/app/main.tsx --bundle --outdir=src/public/assets --loader:.tsx=tsx --platform=browser --jsx=automatic",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "esbuild": "^0.21.5",
    "tsx": "^4.19.0",
    "typescript": "^5.9.2"
  }
}
