{
  "name": "autoloan",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm --workspace apps/web run dev",
    "dev:api": "npm --workspace apps/api run dev",
    "dev:marketplace": "npm --workspace apps/marketplace run dev",
    "dev:scoring": "apps/scoring/.venv/bin/python -m uvicorn apps.scoring.main:app --reload --port 8000",
    "dev:all": "concurrently -n web,api,marketplace,scoring -c blue,green,yellow,magenta \"npm run dev:web\" \"npm run dev:api\" \"npm run dev:marketplace\" \"npm run dev:scoring\""
  },
  "devDependencies": {
    "@prisma/client": "^6.15.0",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "concurrently": "^9.2.1",
    "prisma": "^6.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "zod": "^4.1.5"
  }
}
