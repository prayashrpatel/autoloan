{
  "name": "autoloan",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:api": "npm --workspace apps/api run dev",
    "dev:marketplace": "npm --workspace apps/marketplace run dev",
    "dev:scoring": "npm --workspace apps/scoring run dev",
    "dev:all": "concurrently --continue-on-error -n api,marketplace,scoring -c green,cyan,magenta \"npm run dev:api\" \"npm run dev:marketplace\" \"npm run dev:scoring\""
  },
  "devDependencies": {
    "@prisma/client": "^6.15.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@types/node-fetch": "^2.6.13",
    "concurrently": "^9.2.1",
    "prisma": "^6.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "zod": "^4.1.5"
  }
}
